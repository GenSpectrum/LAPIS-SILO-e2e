{
    "name": "lapis-silo-e2e",
    "version": "1.0.0",
    "description": "Testing the LAPIS API, backed by SILO, designed for easily adding new test-cases.",
    "scripts": {
        "test": "vitest --reporter=html",
        "format": "prettier --write \"./**/*.{ts,tsx,json,astro,md,mdx,mjs,cjs}\"",
        "check-format": "prettier --check \"**/*.{ts,tsx,json,astro,md,mdx,mjs,cjs}\"",
        "type-check": "tsc --noEmit",
        "check": "npm run check-format && npm run type-check"
    },
    "dependencies": {
        "@mongodb-js/zstd": "^1.2.1"
    },
    "type": "module",
    "devDependencies": {
        "@types/node": "^22.5.4",
        "prettier": "^3.3.3",
        "typescript": "^5.5.4",
        "vitest": "^2.0.5",
        "@vitest/ui": "^2.0.5"
    },
    "engines": {
        "node": ">=22.0.0"
    }
}
